{
    "appSettings": {
        "id": "wj-config-ui",
        "title": "React with WJ-Config",
        "projectName": "wj-config by WJ Software",
        "projectHome": "https://github.com/WJSoftware/wj-config"
    },
    "personApi": {
        "initialPersonCount": 100,
        "maxPersonCount": 1000,
        "personCountStep": 10,
        "defaultMinBday": "2000-01-01"
    },
    "ws": {
        "options": {
            "mockaroo": {
                "key": "a53dac10"
            }
        },
        "mockaroo": {
            "host": "my.api.mockaroo.com",
            "scheme": "https",
            "person": {
                "all": "/persons.json?records={numRecords}"
            }
        },
        "flags": {
            "host": "countryflagsapi.com",
            "scheme": "https",
            "flag": "/svg/{countryCode}"
        },
        "countries": {
            "host": "restcountries.com",
            "scheme": "https",
            "search": "/v3.1/alpha?codes={codes}"
        }
    }
}