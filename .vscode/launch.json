{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch NodeJS Example v18.2.0 - v2.0.0",
            "cwd": "${workspaceFolder}/examples/NodeJS/v2.0.0",
            "program": "${workspaceFolder}/examples/NodeJS/v2.0.0/index.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV": "PreProduction",
                "OPT_app__title": "Title set by Environment"
            },
            "type": "node",
            "console": "integratedTerminal"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch React Example v18.2.0 - v1.0.2",
            "url": "http://localhost:3001",
            "webRoot": "${workspaceFolder}/examples/react/v18.2.0/v1.0.2",
            "preLaunchTask": "npm: start - examples/react v18.2.0/v1.0.2"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch React Example v18.2.0 - v1.1.0",
            "url": "http://localhost:3003",
            "webRoot": "${workspaceFolder}/examples/react/v18.2.0/v1.1.0",
            "preLaunchTask": "npm: start - examples/react v18.2.0/v1.1.0"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Express Example v4.16.1 - v1.0.2",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/examples/express/v4.16.1/v1.0.2",
            "program": "${workspaceFolder}/examples/express/v4.16.1/v1.0.2/bin/www",
            "env": {
                "NODE_ENV": "Development"
            },
            "console": "integratedTerminal"
        }
    ]
}